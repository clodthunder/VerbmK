<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/ui_space_5">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上传操作"
        android:textColor="@color/ui_color_black"
        android:textSize="@dimen/ui_font_14"
        app:layout_constraintBaseline_toBaselineOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/npb_upload"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintVertical_chainStyle="packed" />

    <com.lskj.gx.basic_widget.NumberProgressBar
        android:id="@id/npb_upload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/ui_space_5"
        android:layout_marginTop="@dimen/ui_space_5"
        android:layout_marginEnd="@dimen/ui_space_5"
        app:layout_constraintBottom_toTopOf="@+id/tv_file_name_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_title"
        app:progress_current="10"
        app:progress_max="100"
        app:progress_reached_color="@color/ui_color_red"
        app:progress_text_color="@color/ui_color_black"
        app:progress_text_size="@dimen/ui_font_14" />

    <TextView
        android:id="@id/tv_file_name_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/ui_space_5"
        android:layout_marginTop="@dimen/ui_space_5"
        android:layout_marginEnd="@dimen/ui_space_5"
        android:text="名称"
        android:textColor="@color/ui_color_black"
        android:textSize="@dimen/ui_font_14"
        app:layout_constraintBaseline_toBaselineOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/npb_upload"
        app:layout_constraintEnd_toStartOf="@+id/tv_file_name"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/npb_upload"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintVertical_chainStyle="packed" />

    <TextView
        android:id="@id/tv_file_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintHorizontal_weight="1"
        android:layout_marginLeft="@dimen/ui_space_5"
        android:layout_marginEnd="@dimen/ui_space_5"
        android:ellipsize="end"
        android:maxLines="2"
        android:text="asssssssssssssssssssssssssssssssssssssssssssssssssssssssssssssssssssssss"
        android:textSize="@dimen/ui_font_14"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_file_name_label"
        app:layout_constraintBottom_toTopOf="@+id/tv_file_size_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/tv_file_name_label"
        app:layout_constraintTop_toBottomOf="@id/npb_upload" />

    <TextView
        android:id="@+id/tv_file_size_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/ui_space_5"
        android:layout_marginTop="@dimen/ui_space_5"
        android:layout_marginEnd="@dimen/ui_space_5"
        android:text=" 大小"
        android:textColor="@color/ui_color_black"
        android:textSize="@dimen/ui_font_14"
        app:layout_constraintBottom_toTopOf="@+id/btn_cancle_upload"
        app:layout_constraintEnd_toStartOf="@+id/tv_file_size"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_file_name" />

    <TextView
        android:id="@id/tv_file_size"
        android:layout_width="0dp"
        app:layout_constraintHorizontal_weight="1"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/ui_space_5"
        android:layout_marginEnd="@dimen/ui_space_5"
        android:ellipsize="end"
        android:maxLines="1"
        android:text="0kb"
        android:textSize="@dimen/ui_font_14"
        app:layout_constraintBaseline_toBaselineOf="@id/tv_file_size_label"
        app:layout_constraintBottom_toTopOf="@id/btn_cancle_upload"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/tv_file_size_label"
        app:layout_constraintTop_toBottomOf="@id/tv_file_name" />

    <Button
        android:id="@id/btn_cancle_upload"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginStart="@dimen/ui_space_5"
        android:layout_marginTop="@dimen/ui_space_10"
        android:layout_marginEnd="@dimen/ui_space_5"
        android:background="@drawable/btn_cancle_upload"
        android:text="取    消"
        android:textColor="@color/ui_color_black"
        android:textSize="@dimen/ui_font_14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_file_size_label" />
</androidx.constraintlayout.widget.ConstraintLayout>